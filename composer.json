{
    "name": "tostishop/algolia-integration",
    "description": "Algolia search integration for TostiShop WordPress theme",
    "type": "wordpress-theme",
    "require": {
        "algolia/algoliasearch-client-php": "^3.4",
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "TostiShop\\": "inc/"
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "optimize-autoloader": true
    }
}
